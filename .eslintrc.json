{
  "parser": "@babel/eslint-parser",
  "parserOptions": {
    "sourceType": "module",

    "babelOptions": {
      "configFile": "./babel.config.json"
    }
  },
  "extends": ["eslint:recommended", "google"],
  "env": {
    "browser": true,
    "es2021": true
  },
  "rules": {
    "no-prototype-builtins": 0,
    "max-len": ["error", {"ignoreTemplateLiterals": true}]
  },
  "globals": {
    "CanvasToBMP": false,
    "chrome": true,
    "StreamDeckMini": false,
    "StreamDeckV1": false,
    "StreamDeckV2": false
  }
}
