{
  "name": "StreamDeck-Meet",
  "version": "0.7",
  "description": "StreamDeck helper for Google Meet",
  "manifest_version": 2,
  "content_scripts": [
    {
      "matches": ["https://meet.google.com/*"],
      "js": [
        "CanvasToBMP.js",
        "HueLights.js",
        "StreamDeck.js",
        "StreamDeckMini.js",
        "StreamDeckV2.js",
        "MeetWrapper.js",
        "main.js"
      ]
    }
  ],
  "web_accessible_resources": [
    "ico-svg/*"
  ]
}
